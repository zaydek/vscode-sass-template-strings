{
	"name": "sass-template-strings",
	"displayName": "Sass Template Strings",
	"publisher": "ZaydekMichels-Gualtieri",
	"repository": "https://github.com/zaydek/vscode-sass-template-strings",
	"license": "MIT",
	"version": "0.1.0",
	"description": "Syntax highlighting for CSS and Sass template strings and functions",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"sass",
		"javascript",
		"template string",
		"syntax highlighting",
		"react",
		"vue",
		"svelte"
	],
	"engines": {
		"vscode": "^1.5.0"
	},
	"contributes": {
		"grammars": [
			{
				"injectTo": [
					"source.js",
					"source.jsx",
					"source.ts",
					"source.tsx",
					"source.vue",
					"source.svelte"
				],
				"path": "./syntaxes/sass-template.json",
				"scopeName": "sass-template"
			}
		]
	},
	"scripts": {
		"prepare": "node ./node_modules/vscode/bin/install"
	}
}
